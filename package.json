{
  "name": "onlinebookingportal",
  "version": "1.0.0",
  "description": "Website for online booking of Pool/Snooker tables and Playstations",
  "main": "app.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "node app.js",
    "server": "nodemon app.js",
    "client": "npm start --prefix client",
