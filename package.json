    "crypto": "^1.0.1",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-handlebars": "^3.0.0",
    "instamojo-nodejs": "0.0.5",
    "jade": "^1.11.0",
    "jsonwebtoken": "^8.5.0",
    "mongodb": "^3.1.10",
    "node-cron": "^2.0.3",
    "nodemon": "^1.19.4",
    "path": "^0.12.7",
    "pug": "^2.0.3",
    "razorpay": "^2.0.3",
    "react": "^16.7.0",
    "react-script": "^2.0.5",
